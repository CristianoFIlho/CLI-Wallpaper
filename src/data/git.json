{
  "title": "Git",
  "sections": [
    {
      "title": "Repository Management",
      "commands": [
        { "name": "git init", "description": "Initialize new repository" },
        { "name": "git clone <url>", "description": "Clone repository" },
        { "name": "git remote add <name> <url>", "description": "Add remote repository" },
