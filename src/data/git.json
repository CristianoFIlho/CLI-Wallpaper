        { "name": "git stash apply", "description": "Apply stash" },
        { "name": "git stash drop", "description": "Delete stash" }
      ]
    },
    {
      "title": "Configuration",
      "commands": [
        { "name": "git config --global user.name", "description": "Set global username" },
        { "name": "git config --global user.email", "description": "Set global email" },
        { "name": "git config --list", "description": "List configuration" },
        { "name": "git config --global core.editor", "description": "Set default editor" }
