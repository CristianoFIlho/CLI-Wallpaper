{
  "name": "cli-wallpaper-generator",
  "version": "1.0.0",
  "description": "Generate CLI cheat sheet wallpapers in multiple resolutions",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp -r src/data dist/ && cp -r src/templates dist/",
    "dev": "ts-node src/index.ts",
    "generate": "npm run build && node dist/index.js",
    "clean": "rm -rf dist output",
    "server": "npm run build && node dist/server.js",
    "dev:server": "ts-node src/server.ts",
    "start": "npm run server"
  },
  "keywords": [
    "cli",
    "wallpaper",
    "cheat-sheet",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "playwright": "^1.40.0"
  }
}
